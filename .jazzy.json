{
    "output": "api",
    "clean": true,
    "xcodebuild_arguments": ["-project", "ReduxKit/ReduxKit.xcodeproj", "-scheme", "ReduxKit-Mac"],
    "swift_version": "2.1.1",
    "author_name": "ReduxKit",
    "author_url": "https://github.com/ReduxKit/ReduxKit",
    "module_name": "ReduxKit",
    "readme_path": "ReduxKit/README.md",
    "dash_url": "http://reduxkit.github.io/ReduxKit/api/docsets/ReduxKit.xml",
    "root_url": "http://reduxkit.github.io/ReduxKit/",
    "github_url": "https://github.com/ReduxKit/ReduxKit",
    "github_file_prefix": "https://github.com/ReduxKit/ReduxKit/tree/master",
    "assets_directory": "assets",
    "template_directory": "templates",

    "documentation_glob": "ReduxKit/docs/*.md",
    "abstract_glob": "ReduxKit/docs/abstracts/*.md",

    "custom_categories": [{
        "name": "Documentation",
        "children": [
            "Introduction",
            "Getting Started",
            "Redux Compatibility",
            "License"
        ]
    }, {
        "name": "Stores",
        "children": [
            "StoreType",
            "Store",
            "createStore(_:state:)"
        ]
    }, {
        "name": "Disposables",
        "children": [
            "ReduxDisposable",
            "SimpleReduxDisposable"
        ]
    }, {
        "name": "Actions",
        "children": [
            "Action",
            "SimpleStandardAction",
            "StandardAction",
            "DefaultAction",
            "bindActionCreators(_:dispatch:)"
        ]
    }, {
        "name": "Middleware",
        "children": [
            "MiddlewareApiType",
            "MiddlewareApi",
            "applyMiddleware(_:)"
        ]
    }, {
        "name": "Utilities",
        "children": [
            "compose(_:)"
        ]
    }]
}
